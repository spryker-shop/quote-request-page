{
  "name": "spryker-shop/quote-request-page",
  "type": "library",
  "description": "QuoteRequestPage module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker-shop/quote-request-page-extension": "^1.0.0",
    "spryker-shop/shop-application": "^1.0.0",
    "spryker-shop/shop-ui": "^1.0.0",
    "spryker/application": "^3.8.0",
    "spryker/cart": "^5.6.0",
    "spryker/company-user": "^2.6.0",
    "spryker/kernel": "^3.0.0",
    "spryker/quote-request": "^1.0.0",
    "spryker/symfony": "^3.1.0"
  },
  "require-dev": {
    "spryker-shop/cart-page": "*",
    "spryker-shop/customer-page": "*",
    "spryker/code-sniffer": "*",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker-shop/cart-page": "If you want to use cart page molecules: ^1.5.0",
    "spryker-shop/customer-page": "If you want to use customer layout templates: ^1.0.0",
    "spryker/silex": "Use this module when using plugins that need Silex dependencies."
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
